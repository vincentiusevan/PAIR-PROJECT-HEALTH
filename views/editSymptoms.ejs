<%- include('headerDoctors') %>

<main>
    <div class="title">
    <h1>Edit Symptoms</h1>
    <% if (errors) { %>
        <% errors.forEach(err => { %>
        <p class="error"><%= err %> </p>
        <% }) %>
    <% } %>
    </div>
    <form action="/doctors/<%= symptoms.id %>/editSymptoms" method="post">
    <div class="column">
        <label for="">Name Symptoms</label>
        <input type="text" name="name" value="<%= symptoms.name %>">
    </div>
    <div class="column">
        <label for="">Description</label>
        <input type="text" name="description" value="<%= symptoms.description %>"/>
    </div>
    <div class="column">
        <button type="submit">Edit</button>
    </div>
    </form>
</main>

<%- include('footerDoctors') %>
